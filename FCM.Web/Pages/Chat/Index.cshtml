@page
@model FCM.Web.Pages.Chat.IndexModel
@{
}

<div class="pt-3"></div>

<div id="map" style="width: 600px; height: 400px"></div>

<script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
<script src="./leaflet.js"></script>
<script>
    var myGeoJSONPath = './maps.json';
    var myCustomStyle = {
        stroke: false,
        fill: true,
        fillColor: '#fff',
        fillOpacity: 1
    }
    $.getJSON(myGeoJSONPath, function (data) {
        var map = L.map('map').setView([39.74739, -105], 4);

        console.log(map)
        L.geoJson(data, {
            clickable: false,
            style: myCustomStyle
        }).addTo(map);
    })
</script>

